<script is:inline src="/2022/locomotive-scroll.min.js"></script>
<script>
    var croak = new Audio('/2022/croak.ogg');
    croak.volume = 0.2;
    const tooltip = document.querySelector('#tooltip')
    const tooltip1 = document.querySelector('#tooltip1')

   

    function hide() {
        tooltip.style.visibility = 'hidden';
        tooltip1.style.visibility = 'hidden';

    }
    function playCroak() {
        croak.currentTime = 0;
        croak.play();
        console.log("sdasd")
        hide();
    }

    function init() {
        const stickyTop = document.querySelector('#stickyTop')
        const stickyBot = document.querySelector('#stickyBot')
        const textbox = document.querySelector('#stickytext')
        const post = document.querySelector('#poster')
        const r = document.querySelector(':root');
        var w;
        var h;
        const isIOS = /iPad|iPhone|iPod/.test(navigator.userAgent) && !window.MSStream;
        if (isIOS) {
            stickyBot.classList.remove('androidsticky')
        }

        var resizeTimer;

        window.addEventListener('resize', function(event) {

            clearTimeout(resizeTimer);
            resizeTimer = setTimeout(function() {
                w = document.documentElement.clientWidth || window.innerWidth;

                if (w <= 900) {
                    textbox.removeAttribute("data-scroll-sticky");
                    textbox.removeAttribute("data-scroll-target");
                    post.setAttribute("data-scroll-speed","1");

                }  
            }, 200);

        });
        w = document.documentElement.clientWidth || window.innerWidth;
        if (w <= 900) {
            textbox.removeAttribute("data-scroll-sticky");
            textbox.removeAttribute("data-scroll-target");
            post.setAttribute("data-scroll-speed","1");

        } 
        var scroll = new LocomotiveScroll({
            el: document.querySelector('[data-scroll-container]'),
            smooth: true,
           // smartphone: { smooth: true },
            
           
            reloadOnContextChange: true
        });
        scroll.on('scroll', (args) => {
            const elementW = stickyBot.offsetWidth

            const progress = (args.scroll.y / args.limit.y ) 
            const half = progress *(w / elementW) * 100
            stickyBot.style.transform = `translate3d(calc(-50% + ${half}%), 0, 0)`
            stickyTop.style.transform = `translate3d(-${half}%, 0, 0)`

        });


    }

    document.addEventListener("load", init());
</script>
